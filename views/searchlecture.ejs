    <style>
      .searchable-container {
        margin: 20px 0 0 0;
      }
      .searchable-container label.btn-default.active {
        background-color: #007ba7;
        color: #fff;
      }
      .searchable-container label.btn-default {
        width: 90%;
        border: 1px solid #efefef;
        margin: 5px;
        box-shadow: 5px 8px 8px 0 #ccc;
      }
      .searchable-container label .bizcontent {
        width: 100%;
      }
      .searchable-container .btn-group {
        width: 90%;
      }
      .searchable-container .btn span.glyphicon {
        opacity: 0;
      }
      .searchable-container .btn.active span.glyphicon {
        opacity: 1;
      }
    </style>
    <!-- web3 -->
    <script src="../web3/web3.js"></script>
  </head>

  <body class="sidebar-is-reduced">
    <header class="l-header">
      <div class="l-header__inner clearfix">
        <div class="c-header-icon js-hamburger">
          <div class="hamburger-toggle">
            <span class="bar-top"></span><span class="bar-mid"></span
            ><span class="bar-bot"></span>
          </div>
        </div>
        <div class="c-header-icon has-dropdown">
          <span
            class="c-badge
                        c-badge--header-icon animated shake"
            >12</span
          ><i class="fa fa-bell"></i>
          <div class="c-dropdown c-dropdown--notifications">
            <div class="c-dropdown__header"></div>
            <div class="c-dropdown__content"></div>
          </div>
        </div>
      </div>
    </header>
    <div class="l-sidebar">
      <div class="logo"><div class="logo__txt">S</div></div>
      <div class="l-sidebar__content">
        <nav class="c-menu js-menu">
          <ul class="u-list">
            <li class="c-menu__item" data-toggle="tooltip" title="Profile">
              <a class="nav-link" href="/student/dashboard">
                <div class="c-menu__item__inner">
                  <i class="fa fa-file-text-o"></i>
                  <div class="c-menu-item__title"><span>Profile</span></div>
                </div>
              </a>
            </li>
            <li
              class="c-menu__item has-submenu"
              data-toggle="tooltip"
              title="Search Lecture"
            >
              <a class="nav-link" href="/student/search_lecture">
                <div class="c-menu__item__inner">
                  <i class="fa fa-caret-square-o-right"></i>

                  <div class="c-menu-item__title">
                    <span>Search Lecture</span>
                  </div>
                </div>
              </a>
            </li>
          </ul>
        </nav>
      </div>
    </div>

    <section class="wrapper">
          <div class="content">
              <div class="container">
                  <div class="row">
                      <div class="col-xs-12 col-sm-6">
                          <div class="card">
                            <form class="example" autocomplete="off" action="#">
                              <div class="autocomplete" style="width:125%;">
                                <input
                                  id="myInput"
                                  type="text"
                                  name="myTopics"
                                  placeholder="What would you like to learn today?"
                                />
                                <!-- <button type="submit"><i class="fa fa-search"></i></button> -->
                              </div>
                
                              <div class="form">
                                <h4>Availablity</h4>
                                <div class="inputGroup">
                                  <input id="morn" name="morn" type="checkbox" />
                                  <label for="morn">Morning</label>
                                </div>
                
                                <div class="inputGroup">
                                  <input id="after" name="after" type="checkbox" />
                                  <label for="after">Afternoon</label>
                                </div>
                
                                <div class="inputGroup">
                                  <input id="nigh" name="nigh" type="checkbox" />
                                  <label for="nigh">Night</label>
                                </div>
                
                                <div class="inputGroup">
                                  <input id="week" name="week" type="checkbox" />
                                  <label for="week">Weekends</label>
                                </div>
                
                                <h4>Max. rate per hour</h4>
                                
                                <div class="group"  style="width:100%;">
                                  <div class="sidebar">
                                    <input id="ex13" type="text" data-slider-ticks="[10, 20, 30, 40, 50, 60]" data-slider-ticks-snap-bounds="0" data-slider-ticks-labels='["$10", "$20", "$30", "$40", "$50", "$60"]'/>
                                  </div>
                                   </div>
                
                                <h4>Tutor Speaks</h4>
                
                                   <div class="autocomplete" style="width:125%;">
                                      <input
                                        id="myLang"
                                        type="text"
                                        name="myTopics"
                                        placeholder="English, Spanish, French..."
                                      />
                                    </div>
                
                                    <div>
                                    <button type="button" class="btn btn-btmexpert btn-sm sharp  col-sm-offset-4">Find an Expert</button>
                                  </div>
                              </div>
                
                              
                            </form>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
  </section>
